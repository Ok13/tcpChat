<Window x:Class="TcpChat.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="10,0,0.4,-0.2">
        <TextBox HorizontalAlignment="Left" Height="31" TextWrapping="Wrap" VerticalAlignment="Top" Width="166" Name="ipText" Text="192.168.43.5" Margin="10,176,0,0"/>
        <Button Content="Send" HorizontalAlignment="Left" Height="31" Margin="370,176,0,0" VerticalAlignment="Top" Width="138" Click="Button_Click"/>
        <TextBox HorizontalAlignment="Left" Height="103" Margin="10,212,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="340" Name="MessageText" Text="uytr"/>
        <TextBox HorizontalAlignment="Left" Height="131" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="350" VerticalScrollBarVisibility="Auto" IsReadOnlyCaretVisible="True" Name="DialogText"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="370,18,0,0" VerticalAlignment="Top" Name="myIP"/>
        <PasswordBox HorizontalAlignment="Left" Margin="181,176,0,0" VerticalAlignment="Top" Width="169" Height="31" Name="PasswordBox" Password="1234567891234569"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="400,124,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
    </Grid>
</Window>
